<resources>
    <declare-styleable name="RoundDialView">
        <attr name="colorAcceleration" format="color" />
        <attr name="colorVelocity" format="color" />
        <attr name="colorDialBackground" format="color" />
        <attr name="colorBottom" format="color" />
        <attr name="colorCenter" format="color" />
    </declare-styleable>
    <declare-styleable name="RoundDiagramItem">
        <attr name="label" format="string" />
        <attr name="textSize" format="dimension" />
    </declare-styleable>
    <declare-styleable name="RoundDiagramSector">
        <attr name="color" format="color" />
        <attr name="count" format="integer" />
        <attr name="degree" format="integer" />
        <attr name="gap" format="integer" />
        <attr name="gradient" format="integer" />
        <attr name="radius" format="dimension" />
        <attr name="rim" format="dimension" />
        <attr name="rimColor" format="color" />
        <attr name="sectors" format="integer" />
    </declare-styleable>
</resources>
